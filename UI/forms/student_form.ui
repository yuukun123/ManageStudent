                     <property name="styleSheet">
                      <string notr="true">#inputStdDate {
	padding: 0 8px;
	border: 1px solid rgb(148, 148, 148);
	font: 75 9pt &quot;MS Shell Dlg 2&quot;;
	background-color: white;
}

/* Mặc định chữ đen */
#inputStdDate QLineEdit {
	color: rgb(0, 0, 0);
	background: transparent;
	border: none;
}

/* Khi QDateEdit hover thì đổi chữ thành trắng */
#inputStdDate:hover QLineEdit {
	color: rgb(255, 255, 255);
}

/* Khi QDateEdit được focus hoặc click */
#inputStdDate:hover QLineEdit,
#inputStdDate:pressed QLineEdit {
	color: rgb(255, 255, 255);
}

/* Đổi nền khi hover/focus */
#inputStdDate:hover,

#inputStdDate:pressed {
	background-color: rgb(167, 167, 167);
}

/* Nút dropdown */
#inputStdDate::drop-down {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 35px;
    border: none;
    background-color: rgb(94, 173, 161) ;
    image: url(:/UI/icons/chevron-down.svg);
}
</string>
